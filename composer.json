{
    "name": "cxeducativa/autorizacion",
    "description": "Laravel 5 Perfiles y Permisos ",
    "keywords": ["autorizacion", "perfiles", "permisos", "auth", "laravel", "emimaster16"],
    "license": "MIT",
    "authors": [
        {
        	
            "name": "Eminson Mendoza",
            "email": "emimaster16@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/filesystem": "5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/config": "5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Cxeducativa\\Autorizacion\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cxeducativa\\Autorizacion\\AutorizacionServiceProvider"
            ],
            "aliases": {
                "Autorizacion": "Cxeducativa\\Autorizacion\\Facades\\Autorizacion"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "3.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {
             "Cxeducativa\\Autorizacion\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
         "cs": "vendor/bin/phpcs src/*"
    }
}
